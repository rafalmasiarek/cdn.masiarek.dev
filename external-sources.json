{
  "sources": [
    {
      "package": "bootstrap",
      "type": "github-release-assets-semver",
      "repo": "twbs/bootstrap",
      "asset_regex": "bootstrap-.*-dist\\.zip$",
      "extract": [
        {
          "zip_asset_regex": "bootstrap-.*-dist\\.zip$",
          "file_regex": "(^|/)(bootstrap\\.min\\.css)$",
          "out_name": "bootstrap.min.css"
        },
        {
          "zip_asset_regex": "bootstrap-.*-dist\\.zip$",
          "file_regex": "(^|/)(bootstrap\\.bundle\\.min\\.js)$",
          "out_name": "bootstrap.bundle.min.js"
        }
      ],
      "meta": {
        "name": "Bootstrap",
        "homepage": "https://getbootstrap.com/",
        "license": "MIT",
        "source_url": "https://github.com/twbs/bootstrap",
        "author": "The Bootstrap Authors"
      }
    },
    {
      "package": "js-fair-consent",
      "type": "github-release-assets-semver",
      "repo": "rafalmasiarek/js-fair-consent",
      "asset_regex": ".*",
      "zipball_fallback": true,
      "extract": [
        {
          "file_regex": "(^|/)dist/.*\\.min\\.js$",
          "out_name": "js-fair-consent.min.js"
        },
        {
          "file_regex": "(^|/)dist/.*\\.min\\.css$",
          "out_name": "js-fair-consent.min.css"
        },
        {
          "file_regex": "(^|/)dist/.*(?<!\\.min)\\.js$",
          "out_name": "js-fair-consent.js"
        },
        {
          "file_regex": "(^|/)dist/.*(?<!\\.min)\\.css$",
          "out_name": "js-fair-consent.css"
        },
        {
          "file_regex": "(^|/)README\\.md$",
          "out_name": "README.md"
        }
      ],
      "meta": {
        "name": "js-fair-consent",
        "source_url": "https://github.com/rafalmasiarek/js-fair-consent",
        "author": "RafaÅ‚ Masiarek"
      }
    },
    {
      "package": "instagram.css",
      "type": "github-raw-file",
      "repo": "picturepan2/instagram.css",
      "ref": "master",
      "files": [
        {
          "path": "dist/instagram.css",
          "out_name": "instagram.css"
        },
        {
          "path": "dist/instagram.min.css",
          "out_name": "instagram.min.css"
        }
      ],
      "meta": {
        "name": "instagram.css",
        "homepage": "",
        "license": "",
        "source_url": "https://github.com/picturepan2/instagram.css",
        "author": "",
        "readme_url": ""
      }
    },
    {
      "package": "twemoji",
      "type": "github-release-assets-semver",
      "repo": "twitter/twemoji",
      "asset_regex": ".*",
      "zipball_fallback": true,
      "build": {
        "workdir": ".",
        "install": "npm ci --no-audit --no-fund",
        "run": "npm run build",
        "timeout_ms": 600000
      },
      "extract": [
        {
          "file_regex": "(^|/)dist/twemoji\\.js$",
          "out_name": "twemoji.js"
        },
        {
          "file_regex": "(^|/)dist/twemoji\\.min\\.js$",
          "out_name": "twemoji.min.js"
        }
      ],
      "meta": {
        "name": "Twemoji",
        "homepage": "https://twemoji.twitter.com/",
        "license": "MIT",
        "source_url": "https://github.com/twitter/twemoji",
        "author": "Twitter"
      }
    }
  ]
}